{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "aws/aws-sdk-php-laravel": "^3.1",
    "bonroyage/oauth": "1.*",
    "darkaonline/l5-swagger": "^8.5.1",
    "doctrine/dbal": "^2.5",
    "guzzlehttp/psr7": "^1.5",
    "hidehalo/nanoid-php": "^1.1",
    "laravel/framework": "^10.13.2",
    "laravel/helpers": "^1.6",
    "laravel/tinker": "~2.8.1",
    "league/oauth2-client": "^2.4",
    "llagerlof/moodlerest": "^2.3",
    "mews/purifier": "^3.1",
    "php-http/curl-client": "^2.3.0",
    "phpseclib/phpseclib": "2.0.31",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^4.7",
    "sentry/sentry-laravel": "^3.4.1",
    "tymon/jwt-auth": "^2.0",
    "web-token/jwt-core": "^3.2.7",
    "web-token/jwt-key-mgmt": "^3.2.7",
    "web-token/jwt-signature": "^3.2.7",
    "zircote/swagger-php": "^4.7.10"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6",
    "filp/whoops": "~2.0",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~10.2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
